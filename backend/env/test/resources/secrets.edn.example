;; Test environment secrets configuration
;;
;; Use Plaid Sandbox credentials for automated tests.
;; These should be the same sandbox credentials as dev.
;;
;; Copy this to secrets.edn and encrypt with:
;;   cd env/test/resources && age -r $(cat ~/.config/finance-aggregator/key.txt | grep -v "^#" | head -1 | sed 's/.*: //') -o secrets.edn.age secrets.edn && rm secrets.edn

{;; Plaid API credentials - Sandbox for tests
 :plaid {:client-id "your_client_id_here"
         :secret "your_sandbox_secret_here"
         :environment #plaid/environment :sandbox}

 ;; Database encryption key for tests
 :database {:encryption-key nil}}
