;; Development secrets configuration
;;
;; Copy this to secrets.edn and encrypt with:
;;   cd env/dev/resources && age -r $(cat ~/.config/finance-aggregator/key.txt | grep -v "^#" | head -1 | sed 's/.*: //') -o secrets.edn.age secrets.edn && rm secrets.edn
;;
;; Or use the project's bb secrets command from the backend directory.

{;; Plaid API credentials - use Sandbox for development
 :plaid {:client-id "your_client_id_here"
         :secret "your_sandbox_secret_here"
         :environment #plaid/environment :sandbox}

 ;; Database encryption key for encrypting Plaid access tokens
 :database {:encryption-key nil}}
