{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.12.0"}

        ;; HTTP client
        clj-http/clj-http {:mvn/version "3.13.0"}

        ;; JSON/CSV
        com.cnuernber/charred {:mvn/version "1.034"}

        ;; Data serialization
        com.cognitect/transit-clj {:mvn/version "1.0.333"}

        ;; Tabular data processing
        scicloj/tablecloth {:mvn/version "7.029.2"}

        ;; Date/time handling
        tick/tick {:mvn/version "0.7.5"}

        ;; Schema/validation
        metosin/malli {:mvn/version "0.16.4"}

        ;; Database
        datalevin/datalevin {:mvn/version "0.9.22"}

        ;; Web server
        http-kit/http-kit {:mvn/version "2.8.0"}
        metosin/ring-http-response {:mvn/version "0.9.4"}
        ring/ring-core {:mvn/version "1.13.0"}
        ring/ring-json {:mvn/version "0.5.1"}
        ring-cors/ring-cors {:mvn/version "0.1.13"}

        ;; System management
        integrant/integrant {:mvn/version "0.13.1"}

        ;; Routing
        metosin/reitit {:mvn/version "0.7.2"}

        ;; Plaid API
        com.plaid/plaid-java {:mvn/version "25.0.0"}}
 :aliases
 {:test {:extra-paths ["test"]
         :extra-deps {lambdaisland/kaocha {:mvn/version "1.91.1392"}
                      org.clojure/test.check {:mvn/version "1.1.1"}}
         :jvm-opts ["-Djava.library.path=/opt/homebrew/opt/libomp/lib"
                    "-Djna.library.path=/opt/homebrew/opt/libomp/lib"]}

  :dev {:extra-paths ["env/dev/src" "test"]
        :extra-deps {;; REPL and system management
                     integrant/repl {:mvn/version "0.3.3"}
                     org.clojure/tools.namespace {:mvn/version "1.5.0"}

                     ;; Development and debugging
                     vvvvalvalval/scope-capture {:mvn/version "0.3.3"}

                     ;; Testing
                     org.clojure/test.check {:mvn/version "1.1.1"}

                     ;; REPL servers (optional, for editor integration)
                     nrepl/nrepl {:mvn/version "1.5.1"}
                     cider/cider-nrepl {:mvn/version "0.58.0"}}
        :jvm-opts ["-Djava.library.path=/opt/homebrew/opt/libomp/lib"
                   "-Djna.library.path=/opt/homebrew/opt/libomp/lib"]}

  :repl {:extra-paths ["env/dev/src" "test"]
         :extra-deps {integrant/repl {:mvn/version "0.3.3"}
                      org.clojure/tools.namespace {:mvn/version "1.5.0"}
                      vvvvalvalval/scope-capture {:mvn/version "0.3.3"}
                      nrepl/nrepl {:mvn/version "1.5.1"}
                      cider/cider-nrepl {:mvn/version "0.58.0"}}
         :jvm-opts ["-Djava.library.path=/opt/homebrew/opt/libomp/lib"
                    "-Djna.library.path=/opt/homebrew/opt/libomp/lib"]}}}
